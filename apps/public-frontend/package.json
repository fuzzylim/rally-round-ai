    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/supabase-js": "^2.39.8",
    "@vercel/analytics": "^1.5.0",
    "clsx": "2.0.0",
    "date-fns": "^4.1.0",
    "lucide-react": "0.268.0",
